<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/customerListItem"
    android:layout_width="fill_parent" android:layout_height="match_parent" android:background="#21C34C"
    android:paddingTop="5dip">

    <TextView android:id="@+id/businessNameListItem" android:paddingBottom="8dp" android:paddingLeft="4dp"
        android:paddingRight="4dp" android:paddingTop="8dp" android:textColor="@color/white"
        android:textSize="13sp" />
    <LinearLayout android:layout_width="1dp" android:layout_height="match_parent">

        <View style="@style/Divider" />
    </LinearLayout>
    <TextView android:id="@+id/customerAddressListItem" android:paddingBottom="8dp" android:paddingLeft="4dp"
        android:paddingRight="4dp" android:paddingTop="8dp" android:textColor="@color/white"
        android:textSize="13sp" />
    <LinearLayout android:layout_width="1dp" android:layout_height="match_parent">

        <View style="@style/Divider" />
    </LinearLayout>
    <TextView android:id="@+id/customerContactListItem" android:paddingBottom="8dp" android:paddingLeft="4dp"
        android:paddingRight="4dp" android:paddingTop="8dp" android:textColor="@color/white"
        android:textSize="13sp" />

</TableRow>